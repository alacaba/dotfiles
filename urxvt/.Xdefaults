! Fonts
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       none
!Xft.hinting:    true
!Xft.autohint:   true
!Xft.hintstyle:  hintslight
URxvt*letterSpace: -1
URxvt*font: xft:consolas:bold:pixelsize=14
!URxvt*letterSpace: -2.5
!URxvt*font: xft:Liberation Mono for Powerline:regular:pixelsize=15

! misc
URxvt*cursorBlink: true
URxvt*scrollBar: false
!URxvt*keysym.Shift-Control-V: perl:clipboard:paste
URxvt*iso14755: False
URxvt*iconFile: /usr/share/icons/Numix-Circle/scalable/apps/xterm-color.svg

! perl exts
URxvt*perl-ext-common : default,clipboard,matcher,remote-clipboard
URxvt*url-launcher    : xdg-open
URxvt*matcher.button  : 1

! Look up for more colors:
! http://xcolors.net
URxvt*foreground: gray
URxvt*background: #1D1F21
!#1D1F21
!090300
!black
!urxvt*depth: 32
!urxvt*background: rgba:0000/0000/0200/c800
urxvt*visualBell:  false

!URxvt*transparent: true
!URxvt*shading: 30

Emacs.font: Consolas-12
Urxvt.termName: rxvt-unicode

