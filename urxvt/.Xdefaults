! Fonts
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       none
Xft.hinting:    true
Xft.autohint:   true
Xft.hintstyle:  hintslight
URxvt*letterSpace: -1
URxvt*font: xft:consolas:regular:pixelsize=16
!URxvt*letterSpace: -2.5
!URxvt*font: xft:Liberation Mono for Powerline:regular:pixelsize=15

! misc
URxvt*cursorBlink: true
URxvt*scrollBar: false
URxvt*keysym.Shift-Control-V: perl:clipboard:paste
URxvt*iso14755: False
URxvt*iconFile: /usr/share/icons/Numix-Circle/scalable/apps/xterm-color.svg

! perl exts
URxvt*perl-ext-common : default,clipboard,matcher,remote-clipboard
URxvt*url-launcher    : xdg-open
URxvt*matcher.button  : 1

! Look up for more colors:
! http://xcolors.net
URxvt*foreground: white
URxvt*background: #090300
!black
!urxvt*depth: 30
!urxvt*background: rgba:0000/0000/0200/c800
urxvt*visualBell:  false

!URxvt*transparent: true
!URxvt*shading: 30


!black
*color0:  #251f1f
*color8:  #5e5e5e
!red
*color1:  #eb4509
*color9:  #eb4509
!green
*color2:  #94e76b
*color10: #95e76b
!yellow
*color3:  #ffac18
*color11: #ffac18
!blue
*color4:  #46aede
*color12: #46aede
!magenta
*color5:  #e32c57
*color13: #e32c57
!cyan
*color6:  #d6dbac
*color14: #d6dbac
!white
*color7:  #efefef
*color15: #efefef

